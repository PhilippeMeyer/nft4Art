{
    "name": "pos",
    "version": "1.0.0",
    "description": "Pos for Nfts",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "start": "tsc && node index.js",
        "test": "tsc && concurrently -n server,tests --kill-others \"npm start\" \"wait-on tcp:8999 && mocha ./test/*.js\"",
        "test:pos-signin": "tsc && concurrently -n server,tests --kill-others \"npm start\" \"wait-on tcp:8999 && mocha ./test/posLoginManager.js\"",
        "test:app-signin": "tsc && concurrently -n server,tests --kill-others \"npm start\" \"wait-on tcp:8999 && mocha ./test/appLogin.js\"",
        "test:tokenList": "tsc && concurrently -n server,tests --kill-others \"npm start\" \"wait-on tcp:8999 && mocha ./test/tokenList.js\"",
        "test:vote": "tsc && concurrently -n server,tests --kill-others \"npm start\" \"wait-on tcp:8999 && mocha ./test/vote.js\"",
        "test:voteSimul": "tsc && concurrently -n server,tests --kill-others \"npm start\" \"wait-on tcp:8999 && mocha ./test/voteSimul.js\"",
        "local-test": "tsc && mocha ./test/*.js",
        "local-test:pos-signin": "tsc && mocha ./test/posLoginManager.js",
        "local-test:app-signin": "tsc && mocha ./test/appLogin.js",
        "local-test:tokenList": "tsc && mocha ./test/tokenList.js",
        "local-test:vote": "tsc && mocha ./test/vote.js",
        "local-test:voteSimul": "tsc && mocha ./test/voteSimul.js"
    },
